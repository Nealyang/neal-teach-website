{
  "name": "neal-teach-website",
  "version": "1.0.0",
  "description": "neal's website about tech",
  "scripts": {
    "watch-client": "node webpack/webpack-dev-server",
    "clean": "rimraf build static/dist",
    "build-server": "babel src -d build --copy-files",
    "build-client": "webpack --config webpack/prod.config.js",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "start-prod": "cross-env NODE_ENV=production node bin/server",
    "start-api-prod": "cross-env NODE_ENV=production node bin/api.js",
    "start-dev": "nodemon --watch src bin/server.js",
    "start-api-dev": "nodemon --watch src/api  bin/api.js",
    "prod": "npm run build && concurrently -k \"npm run start-api-prod\" \"npm run start-prod\"",
    "dev": "concurrently -k \"npm run watch-client\" \"npm run start-api-dev\" \"npm run start-dev\"",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nealyang/neal-teach-website.git"
  },
  "keywords": [
    "Nealyang",
    "technology",
    "website"
  ],
  "author": "Nealyang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nealyang/neal-teach-website/issues"
  },
  "homepage": "https://github.com/Nealyang/neal-teach-website#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.1",
    "es6-promise": "^4.1.0",
    "express": "^4.13.3",
    "express-session": "^1.15.2",
    "http-proxy": "^1.13.3",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "multer": "^1.3.0",
    "mysql": "^2.13.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.4.1",
    "redux": "^3.2.1",
    "redux-amrc": "^1.0.4",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.2.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "wangeditor": "^2.1.23"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.3.13",
    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "^2.1.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.13.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1",
